<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.kh.finalproject.dao.MenuDAO">
	
	<!-- 메뉴 대분류/중분류 카테고리 -->
	<select id="selectMenuList" resultType="kr.kh.finalproject.vo.MCategoryVO">
		select * from medium_category join large_category on mc_lc_num = lc_num;
		<!-- where mn_mc_num = #{category} -->
	</select>
	<!-- 메뉴 이미지/사진파트 -->
	<select id="selectMainList" resultType="kr.kh.finalproject.vo.MenuVO">
		select mn_name from menu <!-- where mn_num = ${mn_num}  -->
	</select>
</mapper>


